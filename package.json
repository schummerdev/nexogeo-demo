{
  "name": "nexogeo-manus-frontend",
  "version": "1.0.1",
  "description": "Sistema de Gerenciamento de Promoções",
  "main": "index.js",
  "homepage": "/",
  "proxy": "http://localhost:3002",
  "scripts": {
    "start": "react-scripts start",
    "dev:api": "npx dotenv-cli node server-simple.js",
    "dev:full": "concurrently \"npm run dev:api\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --passWithNoTests",
    "test:sprite": "npx @testsprite/testsprite-mcp",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "analyze:bundle": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:analyze": "npm run build && npm run analyze",
    "build:stats": "react-scripts build --stats",
    "performance:audit": "npm run build && npx lighthouse http://localhost:3000 --output json --output-path ./lighthouse-report.json",
    "performance:analyze": "node scripts/performance-analysis.js",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.4.0",
    "concurrently": "^9.2.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "1.9.4",
    "leaflet.heat": "0.2.0",
    "pg": "^8.16.3",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "recharts": "^3.2.1",
    "web-vitals": "^5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "20.x"
  },
  "keywords": [
    "react",
    "promotions",
    "management",
    "frontend"
  ],
  "author": "NexoGeo",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@testsprite/testsprite-mcp": "^0.0.14",
    "cross-env": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
