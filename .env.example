PUBLIC_URL=.
PUBLIC_DIR=../public

# Database Configuration
# Para desenvolvimento local, use sua própria string de conexão PostgreSQL
# Exemplo: postgresql://usuario:senha@localhost:5432/nexogeo_db
DATABASE_URL=postgresql://user:password@localhost:5432/database_name

# JWT Secret - CRÍTICO: Use 128 caracteres hexadecimais
# Gere uma chave forte usando: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
# NOVO EXEMPLO SEGURO: f3d66f17f4cc0e9629a75d86ebccdfd7d7881135116e403f15ea0b1ecf14f5597680f82ddfb38091fee9b43070fdfec28608a042ba1c9a6d1433d60b44f7ab28
JWT_SECRET=your_128_character_hex_jwt_secret_generated_with_crypto_randomBytes

# API Configuration (opcional)
API_PORT=3002
NODE_ENV=development

