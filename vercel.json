{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    { "src": "/api$", "dest": "/api/index.js" },
    { "src": "/api/\\?.*", "dest": "/api/index.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/videos/(.*)" },
    { "src": "/audio/(.*)" },
    { "src": "/imagens/(.*)" },
    { "src": "/static/(.*)" },
    { "src": "/manifest.json" },
    { "src": "/favicon.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}