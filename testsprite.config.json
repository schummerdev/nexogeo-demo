{
  "projectName": "NexoGeo - Sistema de Promoções",
  "testFramework": "jest",
  "testCommand": "npm test",
  "sourceDirectory": "src",
  "testDirectory": "src",
  "testFilePattern": "**/*.test.{js,jsx,ts,tsx}",
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "features": {
    "components": [
      "LoadingComponents",
      "DashboardLayout",
      "Maps/InteractiveMap",
      "CapturaForm"
    ],
    "pages": [
      "DashboardHomePage",
      "ParticipantesPage",
      "PromocoesPage", 
      "MapasPage"
    ],
    "services": [
      "authService",
      "participanteService",
      "promocaoService"
    ]
  },
  "testCategories": [
    "unit",
    "integration", 
    "component",
    "e2e"
  ],
  "environment": "jsdom"
}